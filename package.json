  "keywords": [
    "koa",
    "middleware",
    "router",
    "route"
  ],
  "dependencies": {
    "debug": "^3.1.0",
    "http-errors": "^1.3.1",
    "koa-compose": "^3.0.0",
    "methods": "^1.0.1",
    "path-to-regexp": "0.1.7",
    "urijs": "^1.19.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.10.0",
    "jsdoc-to-markdown": "^1.1.1",
    "koa": "2.2.0",
    "nyc": "^11.6.0",
    "supertest": "^2.0"
